{
    "buildCommand": "cd client && npm install && npm run build",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/signin/(.*)",
            "destination": "/index.js"
        },
        {
            "source": "/signup/(.*)",
            "destination": "/index.js"
        },
        {
            "source": "/messages/(.*)",
            "destination": "/index.js"
        },
    ]
}